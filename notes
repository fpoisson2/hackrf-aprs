#convert wav->raw
find -name "*wav" | xargs basename -s .wav | xargs -i sox -t wav {}.wav -t raw -b 16 -e signed-integer -c 1 -r 22050 {}.raw

#ISS
sox -t wav test/ISSpkt.wav -t raw -b 16 -e signed-integer -c 1 -r 22050 - | python aprs_demod.py -t raw -
python aprs_demod.py -t raw test/ISSpkt.raw
:!clear && python % -t raw test/ISSpkt.raw

