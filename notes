
#interactive verbose mod, no raw output
python aprs_mod.py -v -t null null -t aprs -

#no output, just debug
echo "KI5TOF>APRS:>hello world!" | python aprs_mod.py -v -t null null -t aprs -

#mod + demod
echo "KI5TOF>APRS:>hello world!" | python aprs_mod.py -v -t raw - -t aprs - | multimon-ng -t raw -A -a AFSK1200 -
echo "KI5TOF>APRS:>hello world!" | python aprs_mod.py -v | multimon-ng -t raw -A -a AFSK1200 -
echo "KI5TOF>APRS:>hello world!" | python aprs_mod.py -v | sox -t raw -b 16 -e signed-integer -c 1 -v 1.0 -r 22050 -  -t wav test.wav
atest test.wav
sox -t wav test.wav -t raw -b 16 -e signed-integer -c 1 -r 22050 - | multimon-ng -t raw -A -a AFSK1200 -
sox -t wav test.wav -t raw -b 16 -e signed-integer -c 1 -r 22050 - | python aprs_demod.py -t raw -


echo "KI5TOF>APRS:>hello world!" | python aprs_mod.py -v | python aprs_demod.py -v -t raw -
echo "KI5TOF>APRS,WIDE2-1,WIDE1-1:hello world!" | python aprs_mod.py -v | python aprs_demod.py -v -t raw -
cat aprs.txt | python aprs_mod.py -v | python aprs_demod.py -v -t raw -

gen_packets -r 22050 -o x.wav
sox -t wav x.wav -t raw -b 16 -e signed-integer -c 1 -r 22050 - | python aprs_demod.py -t raw -

#ISS TESTs
wget https://inst.eecs.berkeley.edu/~ee123/sp14/lab3/ISSpkt.wav
#TNC TESTs
wget http://wa8lmf.net/TNCtest/TNC_Test_CD_Ver-1.1.zip
sudo apt-get install bchunk
